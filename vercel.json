{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/nhsa/(.*)",
      "destination": "api/nhsa.ts"
    },
    {
      "source": "/twikoo/(.*)",
      "destination": "api/twikoo.ts"
    },
    {
      "source": "/dailyhot",
      "destination": "/api/dailyhot.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}
